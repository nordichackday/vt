{
    "name": "vt/vt",
    "license": "proprietary",
    "description": "Virtual Timeline",
    "type": "project",
    "require": {
        "php": "~5.5",
        "silex/silex": "~1.2",
        "twig/twig": "~1.16",
        "symfony/finder": "~2.5",
        "symfony/validator": "~2.5",
        "symfony/yaml": "~2.5",
        "symfony/form": "~2.5.0",
        "silex/web-profiler": "~1.0.3",
        "symfony/serializer": "^2.6",
        "doctrine/dbal": "~2.2",
        "symfony/security": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3.3"
    },
    "autoload": {
        "psr-4": {
            "VT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VT\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
